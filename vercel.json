{
  "version": 2,
  "builds": [
    {
      "src": "comp_sys_rankings/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb"}
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "comp_sys_rankings/wsgi.py"
    }
  ]
}